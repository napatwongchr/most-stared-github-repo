{
  "name": "most-stared-github-repo",
  "version": "1.0.0",
  "description": "List most stared github repo",
  "main": "index.js",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "parcel index.html",
    "build": "npm run clear-build-cache && parcel build index.html --public-url ./",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napatwongchr/most-stared-github-repo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/napatwongchr/most-stared-github-repo/issues"
  },
  "homepage": "https://napatwongchr.github.io/most-stared-github-repo/",
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-transform-runtime": "^7.6.2",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@testing-library/jest-dom": "^4.1.2",
    "@testing-library/react": "^9.3.0",
    "babel-jest": "^24.9.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.1.2",
    "jest": "^24.9.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.6.3",
    "@emotion/core": "^10.0.21",
    "axios": "^0.19.0",
    "emotion": "^10.0.17",
    "gh-pages": "^2.1.1",
    "react": "^16.10.2",
    "react-async": "^9.0.0",
    "react-dom": "^16.10.2",
    "uuid": "^3.3.3"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
