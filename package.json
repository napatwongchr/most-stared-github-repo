{
  "name": "most-stared-github-repo",
  "version": "1.0.0",
  "description": "List most stared github repo",
  "main": "index.js",
  "scripts": {
    "clear-build-cache": "rm -rf .cache/ dist/",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "dev": "parcel index.html",
    "build": "npm run clear-build-cache && parcel build index.html --public-url ./",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --u"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/napatwongchr/most-stared-github-repo.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/napatwongchr/most-stared-github-repo/issues"
  },
  "homepage": "https://napatwongchr.github.io/most-stared-github-repo/",
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/plugin-transform-runtime": "^7.11.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.8",
    "babel-jest": "^24.9.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^2.5.1",
    "jest": "^26.2.2",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.19.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.11.1",
    "axios": "^0.21.1",
    "emotion": "^10.0.27",
    "gh-pages": "^2.2.0",
    "react": "^16.13.1",
    "react-async": "^9.0.0",
    "react-dom": "^16.13.1",
    "uuid": "^3.4.0"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 ChromeAndroid versions",
    "last 2 Firefox versions",
    "last 2 FirefoxAndroid versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions",
    "last 2 OperaMobile versions"
  ]
}
